<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Seconddirectory" table="seconddirectory" schema="java_shop">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="directoryName">
            <column name="directoryName" sql-type="varchar(255)"/>
        </property>
        <property name="firstDirectoryId">
            <column name="firstDirectoryId" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>