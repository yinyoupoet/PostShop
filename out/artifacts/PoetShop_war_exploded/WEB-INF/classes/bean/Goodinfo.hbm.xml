<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Goodinfo" table="goodinfo" schema="java_shop">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="goodName">
            <column name="goodName" sql-type="varchar(255)"/>
        </property>
        <property name="stock">
            <column name="stock" sql-type="int(11)"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="-1"/>
        </property>
        <property name="secondDirectoryId">
            <column name="secondDirectoryId" sql-type="int(11)"/>
        </property>
        <property name="saleVolumn">
            <column name="saleVolumn" sql-type="int(11)"/>
        </property>
        <property name="imgPath">
            <column name="imgPath" sql-type="varchar(255)"/>
        </property>
        <property name="publisherId">
            <column name="publisherId" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>